{
  "name": "multi-bot",
  "version": "1.1.0",
  "description": "Um chatbot multi-plataforma em node",
  "main": "app.js",
  "scripts": {
    "build": "set NODE_ENV=production && npm ci",
    "dev": "set NODE_ENV=development && tsc app.js",
    "prod": "set NODE_ENV=production && tsc app.js --prod",
    "build:linux": "export NODE_ENV=production && npm ci",
    "dev:linux": "export NODE_ENV=development && tsc -L app.js",
    "prod:linux": "export NODE_ENV=production && tsc -L app.js --prod"
  },
  "keywords": [
    "bot",
    "whatsapp"
  ],
  "author": "laxeder",
  "license": "MIT",
  "dependencies": {
    "@adiwajshing/baileys": "4.4.0",
    "@types/graceful-fs": "4.1.5",
    "axios": "0.27.2",
    "body-parser": "1.20.0",
    "cors": "2.8.5",
    "express": "4.18.1",
    "graceful": "1.0.2",
    "is-base64": "1.1.0",
    "nonce": "1.0.4",
    "pino": "8.1.0",
    "pino-pretty": "8.1.0",
    "qrcode-terminal": "0.12.0",
    "rxjs": "7.5.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "18.7.14",
    "nodemon": "2.0.19",
    "typescript": "4.8.2"
  }
}
